      "body": {
        "type": "TextualBody",
        "value": "BK: Welcome to this event! Can everyone hear me I assume you can? ",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": "https://zillingworth.github.io/virtual-poem-on-screen/bernadette-mayer-generocity-reading/canvas-1/canvas",
        "selector": {
